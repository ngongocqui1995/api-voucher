{
    "version": 2,
    "name": "api-voucher",
    "installCommand": "yarn install",
    "buildCommand": "yarn build",
    "outputDirectory": ".",
    "builds": [
       { "src": "dist/main.js", "use": "@vercel/node" }
    ],
    "routes": [
       { "src": "/(.*)", "dest": "dist/main.js" }
    ]
 }