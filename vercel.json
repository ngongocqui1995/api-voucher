{
    "version": 2,
    "installCommand": "yarn install",
    "buildCommand": "yarn build",
    "outputDirectory": "dist",
    "builds": [
        {
            "src": "dist/main.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["dist/**"] }
        }
    ],
    "routes": [
       { "src": "/(.*)", "dest": "dist/main.js" }
    ]
 }